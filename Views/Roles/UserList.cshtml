@model IEnumerable<IJustWatched.Models.User>
@using Microsoft.AspNetCore.Mvc.Localization
@inject IViewLocalizer Localizer

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>@Localizer["userList"]</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">
                    @Localizer["rightAccess"]
                </a>
            </td>
        </tr>
    }
</table>
